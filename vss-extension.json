{
    "manifestVersion": 1,
    "id": "microsoft-store-developer-cli-task",
    "name": "Microsoft Store Developer CLI Tasks",
    "version": "0.3.0",
    "publisher": "ms-aps-msstorecli",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Microsoft Store Developer CLI related tasks.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Microsoft/setup-msstore-cli"
    },
    "tags": [
        "MSStoreCLI",
        "MSStoreDevCLI",
        "MSStore",
        "Microsoft Store",
        "Partner Center",
        "Publish",
        "Deploy"
    ],
    "content": {
        "details": {
          "path": "README.md"
        },
        "thirdpartynotice": {
            "path": "licenses.txt"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links":{
        "license": {
            "uri": "https://github.com/microsoft/setup-msstore-cli/blob/main/LICENSE"
        }
    },
    "files": [
        {
            "path": "setup-msstore-cli-task"
        }
    ],
    "contributions": [
        {
            "id": "microsoft-store-developer-cli-setup-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "setup-msstore-cli-task"
            }
        }
    ]
}